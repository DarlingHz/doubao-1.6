{
  "http": {
    "port": 8080,
    "host": "0.0.0.0"
  },
  "database": {
    "path": ":memory:",
    "max_connections": 10
  },
  "cache": {
    "capacity": 1000,
    "ttl_seconds": 3600
  },
  "link": {
    "default_expire_seconds": 86400,
    "short_code_length": 6,
    "max_custom_alias_length": 20
  },
  "log": {
    "level": "INFO",
    "file": "shortlink.log"
  }
}